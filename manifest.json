{
  "manifest_version": 3,
  "name": "Remove Paywallinator",
  "version": "1.2.0",
  "description": "Removes Paywalls from websites",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Remove Paywallinator"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "*://*.no/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.no/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}